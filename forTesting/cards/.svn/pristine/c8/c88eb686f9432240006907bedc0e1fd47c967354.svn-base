            <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="0e8af2f2b5b14137b983be8e5855267e">
                <param name="configFile" value="../../widget_data/config/0e8af2f2b5b14137b983be8e5855267e.json" />
