            <section class="question-long-text" data-uuid="ac11879bb36c46d28721768f377f9571">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="cc01bcad9194413a851801a2088ff577">
                    <param name="configFile" value="../../widget_data/config/cc01bcad9194413a851801a2088ff577.json" />
                </object>
            </section>
