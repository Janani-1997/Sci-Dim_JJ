            <section class="question-long-text" data-uuid="73b4887b516c4453b8a6310a9ba8cf08">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="511720d61dbd4f40aca478204cf8540e">
                    <param name="configFile" value="../../widget_data/config/511720d61dbd4f40aca478204cf8540e.json" />
                </object>
            </section>
