        <section data-uuid="a9bcf30ef1c9427abb26c6c4351d55a2" class="question-multiple-choice">
            <object type="text/html" data="../../assets/widgets/multiple_choice_question/index.html" data-uuid="7051aae984b7411bbf9f40f3b843589c" class="widget">
                <param name="configFile" value="../../widget_data/config/7051aae984b7411bbf9f40f3b843589c.json" />
            </object>
