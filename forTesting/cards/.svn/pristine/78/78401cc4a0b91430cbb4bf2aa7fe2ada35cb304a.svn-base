        <section class="skillbuilder" data-uuid="a6b5c25f93184601bb263a873c805de4">
            <object type="text/html" data="../../assets/widgets/skillbuilder/index.html" class="widget" data-uuid="d92a01fbba2b4b88b0880d3dc96bd41f">
                <param name="configFile" value="../../widget_data/config/d92a01fbba2b4b88b0880d3dc96bd41f.json" />
