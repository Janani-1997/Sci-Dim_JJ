                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="4f93a1c9a4d546b2a9790c7458ef605e">
                    <param name="configFile" value="../../widget_data/config/4f93a1c9a4d546b2a9790c7458ef605e.json" />
