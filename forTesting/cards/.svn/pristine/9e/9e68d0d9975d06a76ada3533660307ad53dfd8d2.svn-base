                <object type="text/html" data="../../assets/widgets/multiple_choice_question/index.html" data-uuid="4547a7c84bc74e528ba3f9bdbd43ce04" class="widget">
                    <param name="configFile" value="../../widget_data/config/4547a7c84bc74e528ba3f9bdbd43ce04.json" />
