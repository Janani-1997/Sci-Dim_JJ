            <section class="question-multiple-choice" data-uuid="20564dec136e4ee2bffcd7519251a99b">
                <object type="text/html" data="../../assets/widgets/multiple_choice_question/index.html" class="widget" data-uuid="8484a22d3e8544e191af522a7148c1ee">
                    <param name="configFile" value="../../widget_data/config/8484a22d3e8544e191af522a7148c1ee.json" />
                </object>
            </section>
