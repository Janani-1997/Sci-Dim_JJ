        <section class="historical-source nobg" data-uuid="32dcf4b33f7d4aeaa21df40c2efc0b64">
            <article class="box gradient" data-uuid="547a475f3a274efbb2e61c13708e300e">
                <div class="header-element" data-uuid="df2c7edf17f5425195a849c6c320d2e1">
