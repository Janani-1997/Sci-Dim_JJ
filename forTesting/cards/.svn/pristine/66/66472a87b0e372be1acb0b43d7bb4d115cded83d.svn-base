            <section class="question-long-text" data-uuid="42afad53889d4ecf8aa0c98319617f4a">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="6d36da185451481d9bde61ade868c324">
                    <param name="configFile" value="../../widget_data/config/6d36da185451481d9bde61ade868c324.json" />
