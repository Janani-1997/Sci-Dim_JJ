                    </object>
                </section>
            </article>
            <section class="question-long-text" data-uuid="b1b85633cabc4b1aadeeb5f920e3f287">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="dc1f5e3b4dd34d51b3425df5973f7947">
                    <param name="configFile" value="../../widget_data/config/dc1f5e3b4dd34d51b3425df5973f7947.json" />
                </object>
            </section>
