            <section class="question-long-text" data-uuid="4ae215a25fac4a62a4c8955b789d1ad4">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="2815d2a2a9ea43bf8f962843c1548cdb">
                    <param name="configFile" value="../../widget_data/config/2815d2a2a9ea43bf8f962843c1548cdb.json" />
                </object>
