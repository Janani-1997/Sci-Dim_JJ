        <section class="historical-source nobg" data-uuid="0f4fd7da7f96493b84c51b10ecd19ec6">
            <article class="box gradient" data-uuid="bc48af3d256c497ba46e0fd65dbc1c3b">
                <div class="header-element" data-uuid="2c0b6f50602543098b44ed39bf3bbf4b">
