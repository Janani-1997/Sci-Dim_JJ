            <section class="question-long-text" data-uuid="d08eee08cf7b466f9888be3b5babacd0">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="b4f7887022e3418393fcffb19c6a100d">
                    <param name="configFile" value="../../widget_data/config/b4f7887022e3418393fcffb19c6a100d.json" />
                </object>
            </section>
