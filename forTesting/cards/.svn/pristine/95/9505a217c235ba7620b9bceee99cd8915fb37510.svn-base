            <section class="question-long-text" data-uuid="46aa7406b2d541daa4d6cdf99d3a54f1">
                <object type="text/html" data="../../assets/widgets/long_text_question/index.html" class="widget" data-uuid="71d5043d89fe4eeeb48d2f7e59e02942">
                    <param name="configFile" value="../../widget_data/config/71d5043d89fe4eeeb48d2f7e59e02942.json" />
                </object>
